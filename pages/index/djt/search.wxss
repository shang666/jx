/* pages/index/djt/search.wxss */
.search_box{
    height:70rpx;
    width:100%;
    background:#137de6;
    font-size:28rpx;
    padding-top:12rpx;
    position: fixed;
    top:0;
    left:0;
    z-index:100
}
.search_box input{
    width:527rpx;
    height:58rpx;
    padding-left:23rpx;
    background:#fff;
    border-radius:6px;
    margin-left:30rpx;
}
.search_box text{
    width:114rpx; 
    height:58rpx;
    margin-right:30rpx;
    background:#4294d8;
    text-align:center;
    line-height:58rpx;
    border-radius:6px;
    color:#fff;
    font-size:28rpx;
}
/* pages/index/djt/djt.wxss */
.dit_box{
    position: relative;
}
.myprolist{
    width:623rpx;
    height:114rpx;
    border:1px solid #ccc;
    border-radius: 6px;
    padding:34rpx 30rpx;
    background: #fff;
    margin:20rpx auto;
}
.list_box{
    padding-top:92rpx; 
}
.selector view{
    width: 683rpx;
    margin: 27rpx auto;
    font-size: 24rpx;
    color: #636363;
    height:36rpx;
}
.selector view text{
    width: 33%;
    text-align: center; 
}
.selector view .fl{
    border-right:1px solid #d7d7db
}
.myprolist .user_img{
    width:113rpx;
    height:113rpx;
    float: left;
    border-radius: 50%;
}
.myprolist .one_box{
    float: left;
    margin-left: 18rpx;
    height:113rpx;
}
.get_block{
    display: block;
    color: #969696;
    font-size:24rpx; 
    width:100%;
}
.one{
    font-size:32rpx;
    width: 210rpx;
    height:38rpx;
    line-height: 38rpx;
}
.one text{
    color: #147ce6;
    display: inline-block;
    font-size:24rpx
}
.start{
    height:16rpx
}
.start image{
    float:left;
    width:16rpx;
    height:16rpx;
    margin:0 5rpx 0 0;
}
.workTypeName{
    height:59rpx;
    line-height:59rpx;
    color: #666;
}
.selector{
    background: #ffffff;
    height:90rpx;
    width: 100%;
    position: fixed;
    top:82rpx;
    z-index:100;
    border-bottom: 1px solid #d3d3d3
}
.shadow{
    position:absolute;
    top:0;
    left:0;
    height:100%;
    width:100%;
    background: #000;
    opacity: 0.5;
    z-index:100;
}
.priv,.workertype,.sort,.city,.xian{
    position: fixed;
    top:92rpx;
    width: 100%;
    background: #fff;
    z-index:101;
    height:0rpx;
    overflow-y: auto;
}
.city,.xian{
    height:606rpx;  
}
.priv text,.city text,.xian text{
    width:95.8%;
    /*padding-left:30rpx;*/
    border-bottom:1px solid #ccc;
    height:85rpx;
    line-height: 85rpx;
   margin-left:30rpx;
   font-size: 24rpx;
}
.workertype text,.sort text{
    width:95.8%;
    /*padding-left:30rpx;*/
    border-bottom:1px solid #ccc;
    height:85rpx;
    line-height: 29rpx;
    margin-left:30rpx;
    font-size: 24rpx;
}
.warning_worker{
    width:450rpx;
    height:250rpx;
    position: absolute;
    left:50%;
    /*margin:-90rpx 0 0 -150rpx;*/
    margin-top:400rpx;
    margin-left:-225rpx;
}
.moreworker{
    width:623rpx;
    background: #fff;
    border:1px solid #ccc;
    border-radius: 6px;
    padding:29rpx 30rpx;
    margin:20rpx auto;
    color: #969696;
}
.moreworker text{
    width: 100%;
    text-align: center;
    font-size:28rpx;
}
